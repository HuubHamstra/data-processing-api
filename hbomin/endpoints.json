  "paths": {
    "/create-profile": {
      "post": {
        "description": "Create a new user profile",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "accountId": {
                    "type": "integer"
                  },
                  "profileName": {
                    "type": "string"
                  },
                  "profileImage": {
                    "type": "string"
                  },
                  "age": {
                    "type": "integer"
                  },
                  "language": {
                    "type": "integer"
                  },
                  "viewMovies": {
                    "type": "integer"
                  },
                  "viewSeries": {
                    "type": "integer"
                  },
                  "minAge": {
                    "type": "integer"
                  },
                  "accept": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "202": {
            "description": "Profile created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "profile": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
